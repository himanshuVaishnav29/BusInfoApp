{
    "version": 2,
    "builds": [
      { "src": "index.js", "use": "@vercel/node" }
    ],
    "routes": [
      { 
        "src": "/clientSide.js", 
        "dest": "/clientSide.js",
        "headers": [
          { "key": "Content-Type", "value": "text/javascript" }
        ]
      },
      { 
        "src": "/static/(.*)", 
        "dest": "/$1"
      },
      { 
        "src": "/(.*)", 
        "dest": "/"
      }
    ]
  }
  