{
    "version": 2,
    "builds": [
      { "src": "index.js", "use": "@vercel/node" }
    ],
    "routes": [
      {
        "src": "/clientSide.js",
        "dest": "/clientSide.js",
        "headers": {
          "Content-Type": "text/javascript"
        }
      },
    {
      "src": "/styles.css", 
      "dest": "/styles.css", 
      "headers": {
        "Content-Type": "text/css"
      }
    }
     ,
     {
        "src": "/logo.jpg", 
        "dest": "/logo.jpg"
      },
      {
        "src": "/static/(.*)",
        "dest": "/$1"
      },
      {
        "src": "/(.*)",
        "dest": "/"
      }
    ]
  }
  