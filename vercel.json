{
  "version": 2,
  "builds": [
    { "src": "index.js", "use": "@vercel/node" }
  ],
  "routes": [
    { "src": "/(.*)", "dest": "/" },
    { "src": "/getAllData", "dest": "/index.js"  },
    { "src": "/getCurrentRequest", "dest": "/index.js"},
    { "src": "/logo.jpg", "dest": "/logo.jpg"},
    { "src": "/styles.css", "dest": "/styles.css"}
    
  ]
}
