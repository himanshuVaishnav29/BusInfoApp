{
    "version": 2,
    "builds": [
      { "src": "index.js", "use": "@vercel/node" }
    ],
    "routes": [
      { "src": "/(.*)", "dest": "/" }
    ],
    "rewrites": [
      { "source": "/clientSide.js", "destination": "/clientSide.js" }
    ],
    "headers": [
      { "source": "/clientSide.js", "headers": [{ "key": "Content-Type", "value": "text/javascript" }] }
    ]
  }
  