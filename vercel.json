{
    "version": 2,
    "builds": [
      { "src": "index.js", "use": "@vercel/node" }
    ],
    "routes": [
      {
        "src": "/clientSide.js",
        "dest": "/clientSide.js",
        "headers": {
          "Content-Type": "text/javascript"
        }
      },
      {
        "src": "/static/(.*)",
        "dest": "/$1"
      },
      {
        "src": "/(.*)",
        "dest": "/"
      }
    ]
  }
  