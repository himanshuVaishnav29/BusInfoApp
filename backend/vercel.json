{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    },
    {
      "src": "^/client/(.*)",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "handle": "filesystem",
      "src": "/client/(.*)"
    },
    {
      "src": "/(.*)",
      "dest": "index.js"
    }
  ]
}
